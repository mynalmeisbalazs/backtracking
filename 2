#include <iostream>
#include <vector>

using namespace std;

int main()
{
    int n,k,x;
    cout << "n=";
    cin >> n;
    vector<int>tomg;
    for(int i=0; i<n; i++){
        cout << "tomb[" << i << "]=";
        cin >> x;
        tomg.push_back(x);
    }
    cout << "k=";
    cin >> k;
    for (int i=0; i<k;i++)
        tomg.pop_back();
    n-=k;
    for (int i=0;i<n;i++)
        cout << tomg[i];
    return 0;
}
